{
  "name": "proyectoAppMovil",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest --runInBand --detectOpenHandles",
    "test-report": "jest --reporters=default --reporters=jest-junit --coverage",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "e2e:build-ios-debug": "detox build -c ios.sim.debug",
    "e2e:build-ios-release": "detox build -c ios.sim.release",
    "e2e:test-ios-debug": "detox test -c ios.sim.debug",
    "e2e:test-ios-release": "detox test -c ios.sim.release",
    "e2e:build-android-debug": "npm run android:clean && detox build -c android.emu.debug",
    "e2e:build-android-release": "npm run android:clean && detox build -c android.emu.release",
    "e2e:test-android-debug": "detox test -c android.emu.debug",
    "e2e:test-android-release": "detox test -c android.emu.release"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.6",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "aws-amplify": "^6.6.4",
    "jest-junit": "^16.0.0",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-reanimated": "^3.15.4",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.27.3",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
